# Local Development Environment Variables
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control

# Database Passwords
NEPTUNE_PASSWORD=your-neptune-password-here
POSTGRESQL_PASSWORD=your-postgresql-password-here

# API Keys
OPENAI_API_KEY=sk-your-openai-api-key-here

# Application Configuration
APP_ENV=local
LOG_LEVEL=debug
EMBEDDING_MODEL_NAME=text-embedding-3-small

# Local Database Connections
VECTORDB_URI=postgresql://postgres:your-postgresql-password-here@localhost:5432/vectordb
KG_URI=ws://localhost:8182/gremlin 